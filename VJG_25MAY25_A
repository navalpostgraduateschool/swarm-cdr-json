    % Callbacks that handle component events
    methods (Access = private)

        % Code that executes after component creation
        function startupFcn(app)
 % Get path to the current app's folder
    imgFolder = fileparts(mfilename('fullpath'));

    % Set default image for Combat Platforms panel
    app.Image_9.ImageSource = fullfile(imgFolder, 'BFV.jpg');

    % Set default image for Combat Support Platforms panel
    app.Image_10.ImageSource = fullfile(imgFolder, 'JLTV.jpg');

    % Set default image for Combat Service Support Platforms panel
    app.Image_11.ImageSource = fullfile(imgFolder, 'FMTV.jpg');
        end

        % Value changed function: CategoryDropDown
        function CategoryDropDownValueChanged(app, event)
  selectedCategory = app.CategoryDropDown.Value;
            
            % Hide all panels first
    app.CombatPlatformsPanel.Visible = "off";
    app.CombatSupportPlatformsPanel_2.Visible = "off";
    app.CombatServiceSupportPlatformsPanel_2.Visible = "off";

    % Show the selected one
    switch selectedCategory
        case "Combat"
            app.CombatPlatformsPanel.Visible = "on";
        case "Combat Support"
            app.CombatSupportPlatformsPanel_2.Visible = "on";
        case "Combat Service Support"
            app.CombatServiceSupportPlatformsPanel_2.Visible = "on";
   
    end

        end

        % Button pushed function: ResetButton
        function ResetButtonPushed(app, event)
            app.HealthEditField.Value=0;
            app.SpeedEditField.Value=0;
            app.FuelLevelEditField.Value=0;
            app.RangeEditField.Value=0;
            app.DetectionRadiusEditField.Value=0;
        end

        % Value changed function: VehicleDropDown
        function VehicleDropDownValueChanged(app, event)
     selectedVehicle = app.VehicleDropDown.Value;
     imgFolder = fileparts(mfilename("fullpath"));

    switch selectedVehicle
        case 'Bradley'
            app.Image_9.ImageSource = fullfile(imgFolder, 'BFV.jpg');
        case 'Abrams'
            app.Image_9.ImageSource = fullfile(imgFolder, 'M1A2.jpg');
        case 'DE M-SHORAD'
            app.Image_9.ImageSource = fullfile(imgFolder, 'DESHORAD.jpeg');
        case 'IFPC-HEL'
            app.Image_9.ImageSource = fullfile(imgFolder, 'IFPCHEL.png');
        otherwise
            app.Image_9.ImageSource = '';  % Clears image if not recognized
    end
        end

        % Value changed function: VehicleDropDown_2
        function VehicleDropDown_2ValueChanged(app, event)
             selectedVehicle = app.VehicleDropDown_2.Value;
    imgFolder = fileparts(mfilename("fullpath"));

    switch selectedVehicle
        case 'JLTV'
            app.Image_10.ImageSource = fullfile(imgFolder, 'JLTV.jpg');
        case 'Paladin'
            app.Image_10.ImageSource = fullfile(imgFolder, 'm109paladin.jpg');
        case 'Stryker'
            app.Image_10.ImageSource = fullfile(imgFolder, 'STRYKERICV.jpg');
        case 'Mortar Carrier'
            app.Image_10.ImageSource = fullfile(imgFolder, 'MORTARCARRIER.jpg');
        case 'ABV'
            app.Image_10.ImageSource = fullfile(imgFolder, 'ABV.jpg');
        otherwise
            app.Image_10.ImageSource = ''; % Clears image if not recognized
    end
        end

        % Value changed function: VehicleDropDown_3
        function VehicleDropDown_3ValueChanged(app, event)
    selectedVehicle = app.VehicleDropDown_3.Value;
    imgFolder = fileparts(mfilename("fullpath"));

    switch selectedVehicle
        case 'FMTV'
            app.Image_11.ImageSource = fullfile(imgFolder, 'FMTV.jpg');
        case 'HMMWV Ambulance'
            app.Image_11.ImageSource = fullfile(imgFolder, 'humveeambulance.jpg');
        case 'Tracked Ambulance'
            app.Image_11.ImageSource = fullfile(imgFolder, 'trackedambulance.jpeg');
        case 'PLS'
            app.Image_11.ImageSource = fullfile(imgFolder, 'PLS.jpg');
        case 'Hercules'
            app.Image_11.ImageSource = fullfile(imgFolder, 'M88HERCULES.jpg');
        otherwise
            app.Image_11.ImageSource = ''; % Clears image if not recognized
    end
        end
    end

    % Component initialization
    methods (Access = private)

        % Create UIFigure and components
        function createComponents(app)

            % Get the file path for locating images
            pathToMLAPP = fileparts(mfilename('fullpath'));

            % Create VehicleJSONGeneratorUIFigure and hide until all components are created
            app.VehicleJSONGeneratorUIFigure = uifigure('Visible', 'off');
            app.VehicleJSONGeneratorUIFigure.Position = [100 100 654 495];
            app.VehicleJSONGeneratorUIFigure.Name = 'Vehicle JSON Generator';

            % Create ImportButton
            app.ImportButton = uibutton(app.VehicleJSONGeneratorUIFigure, 'push');
            app.ImportButton.Position = [388 461 100 22];
            app.ImportButton.Text = 'Import';

            % Create ExportButton
            app.ExportButton = uibutton(app.VehicleJSONGeneratorUIFigure, 'push');
            app.ExportButton.Position = [532 461 100 22];
            app.ExportButton.Text = 'Export';

            % Create ResetButton
            app.ResetButton = uibutton(app.VehicleJSONGeneratorUIFigure, 'push');
            app.ResetButton.ButtonPushedFcn = createCallbackFcn(app, @ResetButtonPushed, true);
            app.ResetButton.Position = [244 461 100 22];
            app.ResetButton.Text = 'Reset';

            % Create CategoryDropDownLabel
            app.CategoryDropDownLabel = uilabel(app.VehicleJSONGeneratorUIFigure);
            app.CategoryDropDownLabel.HorizontalAlignment = 'right';
            app.CategoryDropDownLabel.Position = [25 461 50 22];
            app.CategoryDropDownLabel.Text = 'Category';

            % Create CategoryDropDown
            app.CategoryDropDown = uidropdown(app.VehicleJSONGeneratorUIFigure);
            app.CategoryDropDown.Items = {'(Blank)', 'Combat', 'Combat Support', 'Combat Service Support'};
            app.CategoryDropDown.ValueChangedFcn = createCallbackFcn(app, @CategoryDropDownValueChanged, true);
            app.CategoryDropDown.Position = [78 461 146 22];
            app.CategoryDropDown.Value = '(Blank)';

            % Create CombatPlatformsPanel
            app.CombatPlatformsPanel = uipanel(app.VehicleJSONGeneratorUIFigure);
            app.CombatPlatformsPanel.Title = 'Combat Platforms';
            app.CombatPlatformsPanel.Visible = 'off';
            app.CombatPlatformsPanel.Position = [11 217 621 227];

            % Create Image_9
            app.Image_9 = uiimage(app.CombatPlatformsPanel);
            app.Image_9.Position = [442 41 164 152];
            app.Image_9.ImageSource = fullfile(pathToMLAPP, 'BFV.jpg');

